SubDir HAIKU_TOP src bin desklink ;

SetSubDirSupportedPlatformsBeOSCompatible ;

UsePrivateHeaders app interface shared ;

BinCommand desklink :
	desklink.cpp
	DeskButton.cpp
	MediaReplicant.cpp
	MixerControl.cpp
	VolumeControl.cpp
	VolumeWindow.cpp

	: be libmedia.so $(HAIKU_LOCALE_LIBS) $(TARGET_LIBSUPC++)
	: desklink.rdef
;

DoCatalogs Mail :
	x-vnd.Haiku-desklink
	:
	DeskButton.cpp
	MediaReplicant.cpp
	VolumeControl.cpp
;
